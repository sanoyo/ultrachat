type ChatMessage {
  id: ID!
  message: String!
  createdAt: String!
}

type Space {
  id: ID!
  name: String!
  createdAt: String!
}

type Query {
  getChatMessages: [ChatMessage!]!
}

type Mutation {
  sendMessage(message: String!): ChatMessage!
  createSpace(name: String!): Space!
}

type Subscription {
  messageSent: ChatMessage!
}
