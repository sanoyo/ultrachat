type ChatMessage {
  id: ID!
  message: String!
  createdAt: String!
}

type Query {
  getChatMessages: [ChatMessage!]!
}

type Mutation {
  sendMessage(message: String!): ChatMessage!
}

type Subscription {
  messageSent: ChatMessage!
}

type UserSpace {
  id: ID!
  message: String!
  createdAt: String!
}

type Space {
  spaceID: ID!
  name: String!
}
